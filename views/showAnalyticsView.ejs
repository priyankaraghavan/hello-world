    <script type="text/javascript">
		
		google.charts.load('current', {'packages':['corechart', 'bar']});
		google.charts.setOnLoadCallback(test);
		function test()
		{
			var data = document.getElementById('psi-data').textContent.trim();
			var next= data.split(',');
			var table1 = new google.visualization.DataTable();
			table1.addColumn('string', "Developer");
			table1.addColumn('number', "check-in count")
			var i=0;
			
			for(i=0;i<next.length;i++)
			{
				var row1 =next[i].split(":");				
				table1.addRow([row1[0], parseInt(row1[1])]);				
			}
			
			
			var options = {
				title: 'Check in count for PSI',
				hAxis: {
				  title: 'Developer'				  
				  }
				,
			vAxis: {
			title: 'No of check-ins'
			}
		};

		  var chart = new google.visualization.ColumnChart(
			document.getElementById('chart_div'));

		  chart.draw(table1, options);
						
			}
